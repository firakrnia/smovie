<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No</th>
      <th>Film</th>
      <th>Video</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < video.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= video[i].content?.nama ?? '-' %></td>
          <td><%= video[i].video %></td>
          <td>
            <form action="/video/delete/<%= video[i].id %>?_method=DELETE" method="POST">
              <a href="/video/edit/<%= video[i].id %>" class="btn btn-info btn-sm">
                <i class="fas fa-pencil-alt"></i>
                Edit
              </a>
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Delete
              </button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>